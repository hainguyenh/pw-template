{
  "semi": true,
  "trailingComma": "all",
  "overrides": [
  {
    "files": ["*.json"],
    "options": {
      "trailingComma": "none"
    }
  }
],
  "singleQuote": true,
  "quoteProps": "as-needed",
  "printWidth": 120,
  "tabWidth": 2,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "auto"
}